  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    },
    {
      "name": "SQLC Generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/sqlc-gen-zombiezen/main.go",
    },
    {
      "name": "Env Encrypt",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/envcrypt/main.go",
      "args": [
        "encrypt"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Env Decrypt",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/envcrypt/main.go",
      "args": [
        "decrypt"
      ],
      "cwd": "${workspaceFolder}"
    },
  ]
}